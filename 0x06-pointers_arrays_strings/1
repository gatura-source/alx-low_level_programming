#include "main.h"
#include <string.h>

/**
 * _strncpy - works exacty like strcpy
 * @dest: dest input
 * @src: src input
 * n: chars to be copied
 */

char *_strncpy(char *dest, char *src, int n)
{
	int i;

	dest_len = strlen(dest);

	for (i = 0; i < n && src[i] != '\0'; i++)
	{
		dest[i] = src[i];
	}
	dest[i] = '\0';

	return (0);
}
